{
  "imports": {
    "p-queue": "npm:p-queue@^8.1.0",
    "lodash": "https://cdn.skypack.dev/lodash@4.17.21?dts",
    "moment": "https://esm.sh/moment@2.29.4",
    "gfm": "https://deno.land/x/gfm@0.1.20/mod.ts",
    "octokit": "https://esm.sh/octokit@2.0.14",
    "graphql-schema": "https://esm.sh/@octokit/graphql-schema@13.4.0"
  },
  "fmt": {
    "indentWidth": 2,
    "lineWidth": 120,
    "singleQuote": true,
    "useTabs": false,
    "proseWrap": "preserve",
    "include": ["postprocessing/**/*.ts", "utils/**/*.ts", "typings/**/*.ts"],
    "exclude": ["data/"]
  },
  "compilerOptions": {
    "lib": ["esnext", "dom", "deno.ns"],
    "strict": true
  },
  "unstable": ["sloppy-imports"]
}
